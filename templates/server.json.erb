{
    "bootstrap": <%=node[:consul][:conf][:bootstrap]%>,
    "server": true,
    "datacenter": "<%=node[:consul][:conf][:datacenter]%>",
    "data_dir": "<%=node[:consul][:conf][:data_dir]%>",
    "encrypt": "<%=node[:consul][:conf][:encrypt]%>",
    "log_level": "<%=node[:consul][:conf][:log_level]%>",
    "enable_syslog": <%=node[:consul][:conf][:syslog]%>,
    "retry_join" : <%=node[:consul][:conf][:retry_join]%>,
    "addresses": {
        "https": "<%=node[:consul][:conf][:bind_addr]%>"
    },
    "ports": {
        "https": 8080
    }
}
